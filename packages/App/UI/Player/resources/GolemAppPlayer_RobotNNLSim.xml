          <vertex_position_colour R="255" G="255" B="0" A="255"></vertex_position_colour>
          <vertex_frame_size v1="0.02" v2="0.02" v3="0.02"></vertex_frame_size>
          <edge_colour R="0" G="0" B="0" A="255"></edge_colour>
        </optimised_path_rendererex>
        <collision_bounds>
